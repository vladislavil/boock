          .main__header
            h1.main__title Browse Available Books
            .main__wrapper
              .main__panel
                ul.main__menu
                  li.main__item
                    a(href="#").main__item-link
                      | All Books
                  li.main__item
                    a(href="#").main__item-link
                      | Most Recent
                  li.main__item
                    a(href="#").main__item-link
                      | Most Popular
                  li.main__item
                    a(href="#").main__item-link
                      | Free Books
                .main__search
                  input(type="text" placeholder="Enter Keywords").main__input
                  img(src="images/search.png").main__img-search
